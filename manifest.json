{
  "name": "Vocabulary Thesaurus Search",
  "description": "Use the context menu to lookup definition and visual thesaurus",
  "version": "1.0",
  "manifest_version": 2,
  "options_page": "options.html",
  "permissions": ["contextMenus", "storage"],
  "background": {
    "scripts": [ "locales.js", "background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "options.html"
  },
  "icons": {
   "16": "awesomeVocab16.png",
   "48": "awesomeVocab48.png",
  "128": "awesomeVocab128.png"
 }
}
